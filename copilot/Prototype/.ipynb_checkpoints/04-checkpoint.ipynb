    "class Circle:\n",
    "    def __init__(self, color):\n",
    "        self.color = color\n",
    "\n",
    "    def draw(self):\n",
    "        print(f\"Desenhando um círculo na cor {self.color}\")\n",
    "\n",
    "# Exemplo de uso:\n",
    "meu_circulo = Circle(color=\"vermelho\")\n",
    "meu_circulo.draw()\n"
   ]
